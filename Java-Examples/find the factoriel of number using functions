package github;
import java.util.Scanner;

public class github {
	static int Fact(int b) {
		int result=1;
		for(int i=1;i<=b;i++) {
			result*=i;
		}
		
		return result;
	}
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		System.out.print("Please enter your number:");
		int a=input.nextInt();
		
		System.out.print("Factoriel is : " + Fact(a));
	}

}
