package github;
import java.util.Scanner;

public class github {
	
	public static void main(String[] args) {
		int a=1,b=1,c;
		Scanner input=new Scanner(System.in);
		System.out.print("Enter your number:");
		int n=input.nextInt();
		System.out.print("\t"+a+"\t"+b);
		for(int i=0;i<(n-2);i++) {
			c=a+b;
			System.out.print("\t"+c);
			a=b;
			b=c;
		}
	}
}
